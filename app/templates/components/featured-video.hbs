<div class="col s6 m6 13">
  <div class="card">
    <div class="card-content black-text">
      <span class="card-title small">{{video.videoTitle}}</span>
      <iframe style="position: relative; height: 360px; width: 100%;" src="{{video.videoURL}}" frameborder="0" allowfullscreen></iframe>
      <span class="card-footer small">{{video.presenters}}</span>
    </div>
  </div>
</div>

